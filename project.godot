config_version=5

[application]

config/name="LTF Stopwatch"
config/description="LTF Stopwatch"
config/version="1.0.2"
run/main_scene="res://main.tscn"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.2", "GL Compatibility")
run/low_processor_mode=true
run/delta_smoothing=false
boot_splash/bg_color=Color(0.211765, 0.239216, 0.321569, 1)
boot_splash/image="res://assets/icons/boot_splash.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://assets/icons/icon_white.svg"
config/windows_native_icon="res://assets/icons/application_icon.ico"

[autoload]

HOVER_TIP_BUTTON="*res://scenes/hover_tip/hover_tip_button.tscn"
HOVER_TIP_FOLLOW="*res://scenes/hover_tip/hover_tip_follow.tscn"
GLOBAL="*res://scripts/global.gd"

[display]

window/size/viewport_width=512
window/size/viewport_height=512
window/size/resizable=false
window/size/borderless=true
window/size/transparent=true
window/size/window_width_override=512
window/size/window_height_override=512
window/subwindows/embed_subwindows=false
window/per_pixel_transparency/allowed=true

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://resources/base_theme.tres"
theme/custom_font="res://assets/fonts/space_mono/SpaceMono-Regular.ttf"

