config_version=5

[application]

config/name="LTF Stopwatch"
config/description="A simple Stopwatch in godot"
config/version="v0.8"
run/main_scene="res://main.tscn"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.117647, 0.117647, 0.117647, 1)
config/icon="res://icon.svg"

[autoload]

AL_HoverTipButton="*res://scenes/hover_tip/hover_tip_button.tscn"
AL_HoverTipFollow="*res://scenes/hover_tip/hover_tip_follow.tscn"
GLOBAL="*res://scripts/global.gd"

[display]

window/size/viewport_width=512
window/size/viewport_height=512
window/size/initial_position_type=3
window/size/borderless=true
window/size/transparent=true
window/size/window_width_override=512
window/size/window_height_override=512
window/per_pixel_transparency/allowed=true

[filesystem]

import/blender/enabled=false

